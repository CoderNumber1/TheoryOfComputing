#----------------------------------------------------------------
# Bind a02.o, TG.o
#-------------------------------------------------------------------------
a02:		a02.o TG.o
		g++ -o a02 a02.o TG.o -lm
#-------------------------------------------------------------------------
# Compile main.cpp
#-------------------------------------------------------------------------
a02.o:		main.cpp TG.h
		g++ -g -c main.cpp -o a02.o
#-------------------------------------------------------------------------
# Compile TG.h
#-------------------------------------------------------------------------
TG.o:	TG.cpp TG.h
		g++ -g -c TG.cpp -o TG.o
